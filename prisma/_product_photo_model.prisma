model ProductPhoto {
  id        Int            @id @default(autoincrement())
  product   MedicalProduct @relation(fields: [productId], references: [id], onDelete: Cascade)
  productId Int

  filePath  String
  order     Int            @default(0)

  createdAt DateTime @default(now())

  @@index([productId])
  @@map("product_photos")
}
